{
    "ci": {
        "collect": {
            "url": [
                "https://ksreyan0725.github.io/My_personal_portfolio/",
                "https://ksreyan0725.github.io/My_personal_portfolio/contact.html"
            ],
            "numberOfRuns": 3,
            "settings": {
                "preset": "desktop",
                "throttling": {
                    "rttMs": 40,
                    "throughputKbps": 10240,
                    "cpuSlowdownMultiplier": 1
                }
            }
        },
        "assert": {
            "preset": "lighthouse:recommended",
            "assertions": {
                "categories:performance": [
                    "error",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:accessibility": [
                    "error",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:best-practices": [
                    "error",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:seo": [
                    "error",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:pwa": [
                    "warn",
                    {
                        "minScore": 0.8
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}